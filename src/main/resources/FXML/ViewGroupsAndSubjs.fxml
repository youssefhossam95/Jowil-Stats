<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane fx:id="rootPane" xmlns="http://javafx.com/javafx" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns:fx="http://javafx.com/fxml" >
    <Separator fx:id="midSeparator" orientation="VERTICAL"></Separator>
    <VBox fx:id="groupsTableVbox">
        <Label text="Groups Overview" fx:id="groupsLabel"></Label>
        <JFXTreeTableView fx:id="groupsTable"></JFXTreeTableView>
    </VBox>
    <VBox fx:id="choicesTreeVBox">
        <Label text="Groups Choices" fx:id="treeLabel"></Label>
    </VBox>

    <VBox fx:id="infoVBox">
        <Label fx:id="infoLabel" text="Test Info"></Label>
        <Pane fx:id="gridPaneContainer">
            <GridPane fx:id="infoGridPane"></GridPane>
        </Pane>
    </VBox>
    <Button fx:id="manualButton" text="Manual Configuration">
        <graphic>
            <ImageView fitWidth="13" fitHeight="13">
                <Image url="@../Images/whiteConfig.png">
                </Image>
            </ImageView>
        </graphic>


    </Button>
</AnchorPane>
